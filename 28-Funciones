include <iostream>

using namespace std;

int sumar() {
	return 5+7;
}

string nombreCompleto() {
    
    string nombre = "Ana";
    string apellido = "Aguilar";
	return nombre + "" + apellido;	
}

int main(int argc, char** argv)
{   

    int resultado = sumar ();
    string nombreyApellido = nombreCompleto();
    
    cout<< resultado;
    
    cout<<endl;
    
    cout<<nombreyApellido;
    
	return 0;
	
